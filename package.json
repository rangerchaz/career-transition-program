{
  "name": "career-transition-platform",
  "version": "1.0.0",
  "description": "AI-powered career transition platform with personalized guidance and AI advisors",
  "private": true,
  "scripts": {
    "install": "npm install --prefix backend && npm install --prefix frontend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "db:setup": "cd backend && npm run prisma:migrate && npm run seed",
    "db:studio": "cd backend && npm run prisma:studio",
    "db:migrate": "cd backend && npm run prisma:migrate",
    "db:seed": "cd backend && npm run seed",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up -d"
  },
  "keywords": [
    "career",
    "transition",
    "ai",
    "anthropic",
    "claude",
    "nextjs",
    "express",
    "prisma",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
